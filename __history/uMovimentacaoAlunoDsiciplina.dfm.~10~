  object qrDisciplinaProfessor: TFDQuery
    Connection = dmPrincipal.fdConn
    SQL.Strings = (
      'SELECT nome FROM PROFESSORES  AS p'
      
        'INNER JOIN DISCIPLINAS_PROFESSORES AS d ON d.PROFESSOR_ID = p.ID' +
        ' '
      'INNER JOIN DISCIPLINAS  AS a ON a.ID = d.DISCIPLINA_ID '
      'WHERE a.ID = :disciplina;')
    Left = 168
    Top = 304
    ParamData = <
      item
        Name = 'DISCIPLINA'
        DataType = ftInteger
